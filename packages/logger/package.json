{
  "name": "@whu-court/logger",
  "description": "Logger for the WHU-Court project",
  "version": "0.0.4-beta.7",
  "author": "cstao.cn@icloud.com",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "shx rm -rf dist",
    "dev": "tsc -w",
    "build": "yarn clean && tsc --sourcemap --sourceRoot /node_modules/${npm_package_name}/dist",
    "prepack": "yarn build"
  },
  "files": [
    "dist",
    "!dist/**/*.map"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@whu-court/reporter": "0.0.4-beta.7"
  }
}
