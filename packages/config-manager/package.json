{
  "name": "@whu-court/config-manager",
  "description": "WHU-Court Config Manager",
  "version": "0.1.9",
  "author": "cstao.cn@icloud.com",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
