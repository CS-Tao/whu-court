  "author": "cstao.cn@icloud.com",
  "bin": {
    "wcr": "./bin/run"
  },
  "homepage": "https://github.com/CS-Tao/whu-court",
  "license": "MIT",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "repository": "CS-Tao/whu-court",
  "files": [
    "bin",
    "dist",
    "!dist/**/*.map",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@whu-court/auto-update": "0.1.6-beta.6",
    "@whu-court/config-manager": "0.1.6-beta.6",
    "@whu-court/core": "0.1.6-beta.6",
    "@whu-court/env": "0.1.6-beta.6",
    "@whu-court/github-auth": "0.1.6-beta.6",
    "@whu-court/http": "0.1.6-beta.6",
    "@whu-court/logger": "0.1.6-beta.6",
    "@whu-court/report": "0.1.6-beta.6",
    "@whu-court/utils": "0.1.6-beta.6",
    "cfonts": "^3.1.0",
    "chalk": "4.1.2",
    "cli-table3": "^0.6.2",
    "inquirer": "^8.2.3",
    "listr": "^0.14.3",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "node-notifier": "^10.0.1",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.1",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/node-notifier": "^8.0.2",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "wcr",
    "dirname": "wcr",
    "commands": "./src/commands",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "helpClass": "./src/lib/help",
    "hooks": {
      "init": [
        "./src/hooks/init/printLogo"
      ],
      "prerun": [
        "./src/hooks/prerun/check",
        "./src/hooks/prerun/report"
      ],
      "postrun": [
        "./src/hooks/postrun/goodbye",
        "./src/hooks/postrun/update",
        "./src/hooks/postrun/report"
      ]
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "dev": "tsc -w",
    "start": "./bin/run",
    "clean": "shx rm -rf dist",
    "build": "yarn clean && tsc",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "ENABLE_MOCK=true mocha --forbid-only \"test/**/*.test.ts\""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/CS-Tao/whu-court/issues",
  "keywords": [
    "wcr"
  ],
  "publishConfig": {
    "access": "public"
  },
  "wcrt": {
    "publishDist": [
      "oclif.commands",
      "oclif.helpClass",
      "oclif.hooks.init",
      "oclif.hooks.prerun",
      "oclif.hooks.postrun"
    ]
  }
}
